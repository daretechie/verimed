{
  "name": "@daretechie/verimed-core",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "description": "Open Source Medical Provider Verification API",
  "author": "Dare Adeleke <deeprince2020@gmail.com>",
  "contributors": [
    "VeriMed Open Source Community"
  ],
  "license": "MIT",
  "homepage": "https://github.com/daretechie/verimed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daretechie/verimed.git"
  },
  "bugs": {
    "url": "https://github.com/daretechie/verimed/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test AI_API_KEY=mock jest --config ./test/jest-e2e.json",
    "hash-password": "node scripts/hash-password.js",
    "generate-secrets": "node scripts/generate-secrets.js",
    "generate-license": "ts-node scripts/generate-license.ts",
    "rotate-secrets": "node scripts/rotate-secrets.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d datasource.ts",
    "migration:run": "npm run typeorm -- migration:run -d datasource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d datasource.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.3",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^2.0.0",
    "@types/opossum": "^8.1.9",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.7.4",
    "csv-parse": "^6.1.0",
    "file-type": "^16.5.4",
    "fuse.js": "^7.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "openai": "^6.15.0",
    "opossum": "^9.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "overrides": {
    "glob": "^10.5.0",
    "rimraf": "^5.0.0",
    "inflight": "npm:lru-cache@^10.0.0",
    "@npmcli/move-file": "npm:@npmcli/fs@^3.0.0",
    "test-exclude": "^7.0.0",
    "node-gyp": {
      "npmlog": "npm:console-log-level@^2.0.0",
      "gauge": "npm:console-log-level@^2.0.0",
      "are-we-there-yet": "npm:console-log-level@^2.0.0"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!main.ts",
      "!index.ts",
      "!**/migrations/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
